# server block for plausible.smartworkssoftware.com
server {
    server_name plausible.smartworkssoftware.com;
    listen 80;
    listen [::]:80;

    location / {
        proxy_pass http://127.0.0.1:8000;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    }
}

